use std::collections::HashMap;

#[path = "instrument.rs"]
mod instrument;

pub use instrument::Instrument;

pub struct DataFeed<'a> {
    registry: HashMap<&'a String, &'a instrument::Instrument>,
    name: String
}

impl<'a> DataFeed<'a> {
    pub fn new(name:String) -> DataFeed<'a> {
       DataFeed {
           name,
           registry : HashMap::new()
       }
    }
    pub fn add(&mut self, i:&'a instrument::Instrument) {
        self.registry.insert(i.get_name(),
            i
        );
    }
}
